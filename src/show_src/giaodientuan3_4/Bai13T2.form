            <Property name="text" type="java.lang.String" value="package baitaptuan3_4;&#xa;&#xa;import java.util.Scanner;&#xa;&#xa;public class BT13 {&#xa;    public static Scanner scanner = new Scanner(System.in);&#xa;&#xa;    public static void showMaTrix(int a[][], int n) {&#xa;        for (int i = 0; i &lt; n; i++) {&#xa;            for (int j = 0; j &lt; n; j++) {&#xa;                System.out.print(a[i][j] + &quot; \u005ct &quot;);&#xa;            }&#xa;            System.out.println(&quot;&quot;);&#xa;        }&#xa;    }&#xa;&#xa;    public static void createSprialMaTrix(int n,int a[][]) {&#xa;        int d = 0, i, t = 1;&#xa;        int row = n - 1, col = n - 1;&#xa;        while (t &lt;= n * n) {&#xa;            for (i = d; i &lt;= col; i++) {&#xa;                a[d][i] = t++; &#xa;            }&#xa;            for (i = d + 1; i &lt;= row; i++) {&#xa;                a[i][col] = t++; &#xa;            }&#xa;            for (i = col - 1; i &gt;= d &amp;&amp; t &lt;= n * n; i--) {&#xa;                a[row][i] = t++; &#xa;            }&#xa;            for (i = row - 1; i &gt; d &amp;&amp; t &lt;= n * n; i--) {&#xa;                a[i][d] = t++;  &#xa;            }&#xa;            d++;&#xa;            row--;&#xa;            col--;&#xa;        }&#xa;        showMaTrix(a, n);&#xa;    }&#xa;&#xa;    public static void main(String[] args) {&#xa;        int n;&#xa;        do {&#xa;            System.out.println(&quot;Nhap vao kich thuoc o vuong n*n: &quot;);&#xa;            n = scanner.nextInt();&#xa;        } while (n &lt; 3 || n &gt; 8);&#xa;        int a[][] = new int[n][n];&#xa;        createSprialMaTrix(n, a);&#xa;        &#xa;        System.out.println(&quot;\u005cnVien Dinh Khuong 20183567&quot;);&#xa;    }&#xa;" containsInvalidXMLChars="true"/>
